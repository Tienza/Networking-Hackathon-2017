* * * * * ( sleep 20 ; /bin/sh /var/www/html/includes/getNetworkInformation.sh > /var/www/html/includes/log.txt && /usr/bin/python /var/www/html/includes/python_parse.py )
* * * * * ( sleep 30 ; /bin/sh /var/www/html/includes/getNetworkInformation.sh > /var/www/html/includes/log.txt && /usr/bin/python /var/www/html/includes/python_parse.py )
* * * * * ( sleep 40 ; /bin/sh /var/www/html/includes/getNetworkInformation.sh > /var/www/html/includes/log.txt && /usr/bin/python /var/www/html/includes/python_parse.py )
* * * * * ( sleep 50 ; /bin/sh /var/www/html/includes/getNetworkInformation.sh > /var/www/html/includes/log.txt && /usr/bin/python /var/www/html/includes/python_parse.py )
